{
  "name": "desktop-lftp",
  "productName": "DesktopLftp",
  "version": "0.0.1",
  "description": "A corss platform GUI for LFTP",
  "main": "main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --retries 2 --compilers js:babel-register --recursive --require ./test/setup.js test/**/*.spec.js",
    "test-watch": "npm test -- --watch",
    "test-e2e": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --retries 2 --compilers js:babel-register --require ./test/setup.js ./test/e2e.js",
    "lint": "eslint --ignore-path .gitignore --format=node_modules/eslint-formatter-pretty app test *.js",
    "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=2096 -r babel-register server.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./app/main.development",
    "postinstall": "concurrently \"install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\"",
    "dev": "npm run hot-server -- --start-hot",
    "package": "npm run build && build --publish never",
    "package-win": "npm run build && build --win --x64",
    "package-linux": "npm run build && build --linux",
    "package-all": "npm run build && build -mwl",
    "cleanup": "mop -v"
  },
  "build": {
    "appId": "com.desktoplftp.electron",
    "category": "public.app-category.tools",
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "files": [
      "dist/",
      "node_modules/",
      "app.html",
      "main.js",
      "main.js.map",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage"
      ]
    }
  },
  "directories": {
    "buildResources": "resources",
    "output": "release"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geoffdutton/desktop-lftp.git"
  },
  "author": {
    "name": "Geoff Dutton",
    "email": "g.dutton@gmail.com",
    "url": "https://github.com/geoffdutton"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/geoffdutton/desktop-lftp/issues"
  },
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "react-router",
    "flux",
    "webpack",
    "react-hot",
    "lftp"
  ],
  "homepage": "https://github.com/geoffdutton/desktop-lftp",
  "devDependencies": {
    "asar": "^2.0.1",
    "babel-core": "^6.18.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-tcomb": "^0.4.0",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "boiler-room-custodian": "^0.6.2",
    "chai": "^4.2.0",
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^5.0.6",
    "electron-builder": "^20.44.4",
    "electron-devtools-installer": "^2.0.1",
    "enzyme": "^3.10.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype-errors": "^4.1.0",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-mocha": "^5.3.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fbjs-scripts": "^1.2.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^15.1.1",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "mocha": "^6.1.4",
    "react-addons-test-utils": "^15.3.2",
    "redux-logger": "^3.0.6",
    "sinon": "^7.3.2",
    "spectron": "^7.0.0",
    "style-loader": "^0.23.1",
    "tcomb": "^3.2.15",
    "url-loader": "^2.0.1",
    "webpack": "^4.35.0",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.13.1",
    "webpack-merge": "^4.2.1",
    "webpack-validator": "^3.0.1"
  },
  "dependencies": {
    "bluebird": "^3.4.6",
    "electron-debug": "^3.0.0",
    "font-awesome": "^4.7.0",
    "ftps": "^1.1.1",
    "pty.js": "^0.3.1",
    "react": "^16.8.6",
    "react-desktop": "^0.3.9",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-redux": "^4.0.7",
    "redux": "^4.0.1",
    "redux-electron-store": "^0.6.4",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.1.0",
    "source-map-support": "^0.5.12"
  },
  "devEngines": {
    "node": ">=6.x",
    "npm": ">=3.x"
  }
}
